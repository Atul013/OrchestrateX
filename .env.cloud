# OrchestrateX Cloud Environment Configuration
# Copy this to .env in your project root and update with your MongoDB Atlas details

# ===== DATABASE CONFIGURATION =====
# MongoDB Atlas Connection String (replace with your actual connection string)
MONGODB_CONNECTION_STRING=mongodb+srv://orchestratex_user:YOUR_PASSWORD@orchestratex-cluster.xxxxx.mongodb.net/orchestratex?retryWrites=true&w=majority
DATABASE_NAME=orchestratex

# ===== APPLICATION SETTINGS =====
APP_ENV=production
API_KEYS_MASTER=prod-master-key-change-this
REQUEST_TIMEOUT_SECONDS=20
PROVIDER_TIMEOUT_SECONDS=15
MAX_TOKENS_PER_REQUEST=2000
RATE_LIMIT_RPS=5
DEFAULT_MODEL=

# ===== AI PROVIDER CONFIGURATIONS =====
